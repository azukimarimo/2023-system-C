#include <stdio.h>

int main(void) {
    printf("Hello Programming in World!\n");
    return 0;
}



#include <stdio.h>

int main(void)
{
    /* メッセージを表示します */
    printf("Hello Programming in C World!\n");

    return 0;
}



#include <stdio.h>

int main(void)
{
    /* コメントが長くなる場合は
       このように複数行に書くこともできます */
    printf("Hello Programming in C World!\n");

    return 0;
}



#include <stdio.h>

int main(void)
{
    /* メッセージを表示します */
    printf("Hello Programming in C World!\n");
    printf("Good-bye Programming in C World!\n");
    
    return 0;
}



#include <stdio.h>

int main(void)
{
    /* メッセージを表示します */
    /* Helloを表示しないように変更する
    printf("Hello Programming in C World!\n);   /*Helloを表示 */
    */
    printf("Good-bye Programming in C World!\n"); /* Good-byeを表示　*/

    return 0;
}



#include <stdio.h>

int main(void)
{
    // メッセージを表示します
    /* Helloを表示しないように変更する
    printf("Hello Programming in C World!\n");   //Helloを表示
    */
    printf("Goob-bye Programming in C World!\n"); // Goob-byeを表示

    return 0;
}



int main(void)
{
    int a, b, c;
    a = 5;
    b = 3;
    c = a + b;
    printf("%d\n", c);
    
    return 0;
}
